{
  "name": "typescript-express-project",
  "version": "1.0.0",
  "description": "A TypeScript Express project",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register src/app/index.ts",
    "build": "tsc",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:generate": "prisma generate",
    "prisma:test:migrate": "cross-env DATABASE_URL=postgresql://postgres:postgres@localhost:5432/web_app_db_integration_test?schema=public npx prisma migrate dev --name init",
    "test": "cross-env DOTENV_CONFIG_PATH=backend/.env.test jest --config jest.config.js"
  },
  "dependencies": {
    "@prisma/client": "^5.19.1",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "helmet": "^7.1.0",
    "http-status-codes": "^2.3.0",
    "inversify": "^6.0.2",
    "reflect-metadata": "^0.2.2",
    "supertest": "^7.0.0",
    "uuid": "^10.0.0",
    "winston": "^3.14.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.10.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.13",
    "@types/node": "^16.18.108",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.10.0",
    "globals": "^15.9.0",
    "jest": "^29.7.0",
    "nodemon": "^2.0.15",
    "prisma": "^5.19.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.9.5",
    "typescript-eslint": "^8.5.0"
  }
}
